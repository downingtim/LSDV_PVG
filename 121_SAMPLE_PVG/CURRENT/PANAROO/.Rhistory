install.packages("igraph")
library("igraph")
g = read.graph(file="final_graph.gml", format="gml")
summary(g)
vcount(g)
ecount(g)
pdf("view_gml.pdf")
plot(g, layout=layout.fruchterman.reingold(g), vertex.label=NA, vertex.size=5)
dev.off()
V(g)$label
q()
